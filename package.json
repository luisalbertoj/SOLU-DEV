{
  "name": "solu-dev",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve $PROJECT_NAME",
    "build": "nx build",
    "heroku-postbuild": "nx build $PROJECT_NAME --prod",
    "test": "nx test",
    "test-coverage-leads-up": "npx nx test leads-up --code-coverage",
    "test-coverage-api": "nx run api:test --code-coverage=true",
    "test:ci": "ng run-many --target=test --all --parallel --coverage --coverageReporters=lcov && node ./tools/coverageMerger.js",
    "e2e": "nx run leads-up-e2e:e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx nx lint leads-up && npx nx lint api",
      "pre-push": "npx nx test leads-up --code-coverage && nx run api:test --code-coverage=true && nx run leads-up-e2e:e2e"
    }
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/cdk": "14.1.2",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "14.1.2",
    "@angular/forms": "~14.0.0",
    "@angular/localize": "^14.0.6",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@nestjs/common": "9.0.11",
    "@nestjs/core": "9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@ngxs/store": "^3.7.4",
    "@nrwl/angular": "14.5.6",
    "mongoose": "^6.4.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primeng": "^14.0.0",
    "prismjs": "^1.28.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
